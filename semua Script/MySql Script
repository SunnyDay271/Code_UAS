CREATE DATABASE Game_Trade;

USE Game_Trade;

CREATE TABLE Game_Items(
    id int(10) AUTO_INCREMENT NOT NULL,
    item_name varchar(200) NOT NULL,
    game varchar(155) NOT NULL,
    price int(10),
    quantity int(10),
    trader_name varchar(100),
    user_id int(10)
);

INSERT INTO game_items(item_name, game, price, quantity,trader_name,user_id) VALUES
("pedang berlian", "minecraft", 50.00, 2, "ItzKevinz", 112233); 

CREATE TABLE users (
    id INT AUTO_INCREMENT PRIMARY KEY,
    name VARCHAR(100),
    email VARCHAR(100) UNIQUE,
    avatar TEXT,
    password varchar(255)
);
